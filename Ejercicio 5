#include <stdio.h>
int main (){
    int n1, n2, suma, i;
    printf ("\n   Introduzca primer numero (entero): ");
    scanf ("%d", &n1);
    printf ("\n   Introduzca segundo numero (entero): ");
    scanf ("%d", &n2);
    suma = 0;
    i = n1;
    while(i<=n2){
        //sumar los nÃºmeros
        suma = suma + i;
        i++;
    }
    // Imprimir el resultado
    printf("La suma de los valores entre %d y %d es: %d", n1, n2, suma);
    return 0;
}
